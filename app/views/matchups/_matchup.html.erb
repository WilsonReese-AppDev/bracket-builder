id: <%= matchup.id %>
round: <%= matchup.round %>
rp: <%= matchup.round_position %>

<% if matchup.bracket.status == "created" %>
  <%= render "matchups/matchup_created", matchup: matchup %>
<% elsif matchup.bracket.status == "in_progress" %>
  <%= render "matchups/matchup_in_progress", matchup: matchup %>
<% end %>


